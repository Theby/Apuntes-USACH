    <section id="title" class="shinyOrange">
      <div class="container">
        <div class="row">
          <div class="col-md-6">
            <h1>Lista de Entries</h1>
          </div>
        </div>
      </div>
    </section>

    <section id="body" class="officialGrey">
      <div class="container allWhite">
        <div class="table-responsive">
          <table class="table">
            <thead>
              <tr>
                <th>Nombre</th>
                <th>Fecha</th>
                <th>Linkmd</th>
                <th>Publicacion</th>
                <th colspan="3"></th>
              </tr>
            </thead>

            <tbody>
              <% @entries.each do |entry| %>
                <tr>
                  <td><%= entry.nombre %></td>
                  <td><%= entry.fecha %></td>
                  <td><%= entry.linkMD %></td>
                  <td><%= entry.publicacion.nombre %></td>
                  <td><%= link_to 'Show', entry, :class => "btn btn-warning" %></td>
                  <td><%= link_to 'Edit', edit_entry_path(entry), :class => "btn btn-warning" %></td>
                  <td><%= link_to 'Destroy', entry, method: :delete, data: { confirm: 'Are you sure?' }, :class => "btn btn-warning" %></td>
                </tr>
              <% end %>
            </tbody>
          </table>

          <br>

          <%= link_to 'New Entry', new_entry_path, :class => "btn btn-info" %>

          <br><br>
      </div>
    </section>

    <section id="footer">
      <div class="container navbar-fixed-bottom">
        <div class="panel panel-success">         
          <div class="panel-heading">
            <h3 class="panel-title" align="center"> Desarrollado por Esteban Gaete Flores </h3>
          </div>
        </div>
      </div>
    </section>