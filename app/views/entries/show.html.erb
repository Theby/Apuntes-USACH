<p id="notice"><%= notice %></p>

    <section id="title">
      <div class="container">
        <div class="row">
          <div class="col-md-6">
            <h3>
              <strong>Nombre:</strong>
              <%= @entry.nombre %>
            </h3>
          </div>
        </div>
      </div>
    </section>

    <section id="body">
      <div class="container">
		<p>
		  <strong>Fecha:</strong>
		  <%= @entry.fecha %>
		</p>

		<p>
		  <strong>Linkmd:</strong>
		  <%= @entry.linkMD %>
		</p>

		<p>
		  <strong>Publicacion:</strong>
		  <%= @entry.publicacion.nombre %>
		</p>

		<%= link_to 'Edit', edit_entry_path(@entry), :class => "btn btn-link" %> |
		<%= link_to 'Back', entries_path, :class => "btn btn-link" %>
      </div>
    </section>

    <section id="footer">
      <div class="container navbar-fixed-bottom">
        <div class="panel panel-success">         
          <div class="panel-heading">
            <h3 class="panel-title" align="center"> Desarrollado por Esteban Gaete Flores </h3>
          </div>
        </div>
      </div>
    </section>